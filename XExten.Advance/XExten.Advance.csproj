<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net6</TargetFrameworks>
		<Authors>EmilyEdna</Authors>
		<Description>XExten包的优化升级版本，后期只会更新这个包，不在维护XExten包</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>XExten.Advance</PackageId>
		<Product>XExten.Advance</Product>
		<Version>1.2.3-preview</Version>
		<AssemblyVersion>1.2.3</AssemblyVersion>
		<PackageProjectUrl>https://github.com/EmilyEdna/XExten.Advance</PackageProjectUrl>
		<FileVersion>1.2.3</FileVersion>
		<AssemblyName>XExten.Advance</AssemblyName>
		<RootNamespace>XExten.Advance</RootNamespace>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DocumentationFile>D:\Project\XExten.Advance\XExten.Advance\XExten.Advance.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="CacheFramework\MongoDbCache\**" />
		<Compile Remove="CacheFramework\RedisCache\**" />
		<Compile Remove="InternalFramework\Office\**" />
		<EmbeddedResource Remove="CacheFramework\MongoDbCache\**" />
		<EmbeddedResource Remove="CacheFramework\RedisCache\**" />
		<EmbeddedResource Remove="InternalFramework\Office\**" />
		<None Remove="CacheFramework\MongoDbCache\**" />
		<None Remove="CacheFramework\RedisCache\**" />
		<None Remove="InternalFramework\Office\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="AutoMapper" Version="11.0.1" />
		<PackageReference Include="Chinese" Version="0.5.0" />
		<PackageReference Include="DnsClient" Version="1.6.0" />
		<PackageReference Include="DryIoc.dll" Version="4.8.8" />
		<PackageReference Include="Mapster" Version="7.3.0" />
		<PackageReference Include="MessagePack" Version="2.3.85" />
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
		<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="6.0.1" />
		<PackageReference Include="Microsoft.Extensions.DependencyModel" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.FileProviders.Physical" Version="6.0.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="Polly" Version="7.2.3" />
		<PackageReference Include="RestSharp" Version="107.3.0" />
		<PackageReference Include="System.Runtime.Loader" Version="4.3.0" />
		<PackageReference Include="System.Text.Encoding.CodePages" Version="6.0.0" />
		<PackageReference Include="System.Threading.Channels" Version="6.0.0" />
	</ItemGroup>

</Project>
