<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net9</TargetFrameworks>
		<Authors>EmilyEdna</Authors>
		<Description>XExten包的优化升级版本，后期只会更新这个包，不在维护XExten包</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>XExten.Advance</PackageId>
		<Product>XExten.Advance</Product>
		<Version>2.2.3.3</Version>
		<AssemblyVersion>2.2.3.3</AssemblyVersion>
		<PackageProjectUrl>https://github.com/EmilyEdna/XExten.Advance</PackageProjectUrl>
		<FileVersion>2.2.3.3</FileVersion>
		<AssemblyName>XExten.Advance</AssemblyName>
		<RootNamespace>XExten.Advance</RootNamespace>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
